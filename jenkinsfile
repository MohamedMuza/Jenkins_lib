@Library('shared-library') _

pipeline{
    agent any
    stages{
        stage('Demo'){
            steps{
                script
                {       dir('BUILD'){ 
                cleanWs()
                    sh """git clone https://github.com/davidmoten/maven-demo.git
                    cd maven-demo """                
                    }
                    dir ('BUILD/maven-demo')
                    {
                    mavenbuild()
                    }
                }
            }
        }
    }
}
